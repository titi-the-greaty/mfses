{
  "buildCommand": null,
  "framework": null,
  "rewrites": [
    { "source": "/dashboard", "destination": "/dashboard/index.html" },
    { "source": "/dashboard/(.*)", "destination": "/dashboard/index.html" },
    { "source": "/(.*)", "destination": "/homepage/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
